{
  "name": "parse-open-graph",
  "version": "0.4.0",
  "description": "Open Graph parser for browser",
  "main": "dist/openGraph",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasha-io/parse-open-graph.git"
  },
  "keywords": [
    "open",
    "graph"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kasha-io/parse-open-graph/issues"
  },
  "homepage": "https://github.com/kasha-io/parse-open-graph#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.8.0",
    "eslint-config-enough": "^0.3.8",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  }
}
