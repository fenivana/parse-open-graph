{
  "name": "parse-open-graph",
  "version": "0.2.4",
  "description": "Open Graph parser for browser",
  "main": "dist/openGraph.umd.js",
  "module": "dist/openGraph.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/parse-open-graph.git"
  },
  "keywords": [
    "open",
    "graph"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/parse-open-graph/issues"
  },
  "homepage": "https://github.com/fenivana/parse-open-graph#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.19.1",
    "eslint-config-enough": "^0.3.3",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4"
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  }
}
